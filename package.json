{
  "name": "datamap-generator",
  "version": "1.0.8",
  "description": "data map generator for oyster",
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --colors",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "yarn build"
  },
  "author": "Jared Farago",
  "license": "MIT",
  "dependencies": {
    "js-sha3": "^0.7.0",
    "lodash": "^4.17.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-preset-es2015": "^6.24.1",
    "iota.lib.js": "^0.4.7",
    "jest": "^22.4.3",
    "node-forge": "^0.7.5",
    "regenerator-runtime": "^0.11.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    }
  }
}
